                        </h4>
                    </div>
                </div>
                <div class="modal-body">
                    <p>
                        A contract is exposed to a reentrant method call if it makes state changes after calling another
                        contract.
                    </p>
                    <div class="margin-top-30 modal-code-area" data-lines="22">
                  <textarea rows="15" cols="60" name="code-dialog" style="display: none;">
  contract Token {
      mapping(address => uint) balances;
      uint totalBalance;

      function mint(address to, uint amount) {
          balances[to] += amount;
          totalBalance += amount;
      }

      function total() returns(uint) {
          return totalBalance;
      }
  }

  contract Crowdsale {
      Token token;
      bool finished;
      uint hardcap;

      function sell(address to, uint amount) {
          require(!finished);
          token.mint(to, amount);
          if (token.total() >= hardcap) {
              finished = true;
          }
      }
  }
                    </textarea>
                    </div>
                    <p class="margin-top-30">
                    </p>
                </div>
            </div>
        </div>
