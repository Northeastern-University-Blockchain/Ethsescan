<body>
<div class="overflow-container">
    <!-- section-1 -->

    <section class="section-2 container-fluid">
        <div class="content-container">
            <div class="container input-container z-index10">
                <div class="contract-list">
                    <div class="analysis-progress">
                        <div class="analysis-progress__counter">
                            <span class="analysis-progress__done"></span>
                            <span class="analysis-progress__total"></span>
                            <span class="analysis-progress__initial">点击按钮扫描合约</span>
                        </div>
                    </div>
                    <div class="analysis-progress__list hidden">
                      <span class="progress-list__item">
                          <span class="progress-list__item-wrapper--initial">
                              <i class="far fa-file" aria-hidden="true"></i>
                              <span>Contract1</span>
                          </span>
                      </span>
                    </div>
                </div>
                <div class="editor-wrapper">
                    <div class="buttons-editor">
                        <div class="demo-container">
                            <form class="scan-now-form form-inline" action="#">
                                <button href="javascript:void(0)" id="run_btn"
                                        class="btn btn_light_blue btn-default z-index10">
                                    开始检测
                                </button>
                                <a data-toggle="modal" data-target="#info-scan-now"
                                   href="javascript:void(0)">
                                    <i class="fas fa-info-circle"></i>
                                </a>
                            </form>
                        </div>
                        <div class="upload-type-buttons-container" style="display: none">
                            <button href="javascript:void(0)" id="paste_code"
                                    class="btn btn-default demo upload-type-button">
                                Paste Code
                            </button>
                            <button href="javascript:void(0)" id="upload_files"
                                    class="btn btn-default demo upload-type-button">
                                Upload Zip
                            </button>
                            <button href="javascript:void(0)" id="clone_git"
                                    class="btn btn-default demo upload-type-button">
                                Clone Git
                            </button>
                        </div>
                    </div>
                    <div id="code-input__text" class="codeholder-container">
                        <div class="codeholder">
                      <textarea rows="32" cols="120" id="code-main" name="code-main" style="display: none;">
</textarea>
                        </div>

                        <div id="code-input__zip" class="upload-type">
                            <div class="file-upload-container">
                                <form id="zip-form" class="file-upload-form" name="content">
                                    <p>
                                        <i class="fas fa-cloud-download-alt"></i><br><br>
                                        <span id="drap-drop-text">Click to upload File or Drag & Drop here</span></p>
                                    <input type="file" name="content" id="zipUpload">
                                </form>
                                The ZIP file must contain all contracts
                            </div>
                        </div>
                        <div id="code-input__git" class="upload-type">
                            <form class="form-inline" action="#">
                                <div class="form-group input-field">
                                    <input placeholder="Paste link to your Git repository"
                                           class="form-control repository smooth-transition" id="git_url"
                                           value="https://github.com/vkonst/ctd-token.git">
                                    Support for git repositories is experimental
                                </div>
                            </form>
                        </div>
                        <div class="terms-and-condition" align="center">
                            <a data-toggle="modal" data-target="#modal-terms" href="javascript:void(0)">使用本软件，您将接受相关条款</a>
                        </div>
                        <!-- code placeholder -->
                    </div>
                </div>
            </div>
            <div id="div_for_iframe">
                <div class="upload-error__title">分析结果展示</div>
                <iframe id="iframe_chart" src="/static/demo.html" width="1200px" height="650px"   frameborder="0"  name="myiframe" scrolling="yes" allowtransparency="true" border="0">
                </iframe>
            </div>
            <div class="upload-error">
                <div class="upload-error__title">Oops!</div>
                <div class="upload-error__message">Compiler error: ...</div>
                <div class="upload-error__info">
                  <textarea>

                  </textarea>
                </div>
            </div>

            <div id="analyzeResult" class="upload-error">
                <div class="upload-error__title">分析结果展示</div>
                <!--<div class="upload-error__message"></div>-->
                <div class="upload-error__info">
                  <textarea id="resultContent">

                  </textarea>
                </div>
            </div>

            <div class="trim-container">
                <div class="trim trim-white"></div>
            </div>
        </div>
    </section>
    <!-- section-2 -->


    <section id="content_analysis" class="section-4 container-fluid fade in padding-comeback section-blue hidden">
        <div class="content-container">
            <div class="container ">
                <div class="section-4-container">
                    <h2 class="section-4-head"><span>Security Report</span>
                        <a class="legend-link" data-toggle="modal" data-target="#modal-legend"
                           href="javascript:void(0)">
                            <i class="fas fa-info-circle"></i>
                        </a>
                    </h2>
                    <a class="feedback" data-toggle="modal" data-target="#feedback-form" href="javascript:void(0)">
                        <i class="far fa-comment"></i>
                        <span>
                      Tell us how to improve
                      </span>
                    </a>
                    <div class="col-xs-12 col-sm-6 padding-0 margin-top-20 security-items-container">
                        <p class="legend-link-wrapper">

                        </p>
                        <div class="panel-group" id="accordion">
                            <div class="total-issues-container">
                                <div id="issues-found-container">
                                    <span class="total-text uppercase">Total </span>
                                    <span class="total-text">issues</span>
                                    <span class="total-text total-count total-issue-count"></span>
                                </div>
                                <div id="no-issues-found-container">
                                    <span class="total-text">No issues found</span>
                                </div>
                            </div>
                            <div class="report-subcontainer">
                                <hr class="report-hr">
                                <span class="report-subcontainer-text">Transaction Reordering</span>
                                <span class="warning-text total-count transaction-reordering-count"></span>
                            </div>
                            <div id="vul_list_TODTransfer" class="panel panel-default expanded">
                                <a class="info" data-toggle="modal" data-target="#modal-TODTransfer" href="#">info</a>
                                <div class="panel-heading" data-toggle="collapse" href="#collapse1">
                                    <h4 class="panel-title expand">
                                        <div class="right-arrow pull-right">
                                            <span class="glyphicon glyphicon-menu-up"></span>
                                        </div>
                                        <span>Transactions May Affect Ether Transfer</span>
                                    </h4>
                                </div>
                                <div id="collapse1" class="panel-collapse collapse in" aria-expanded="true">
                                    <p class="penal-item description">
                                        Ether transfers whose execution can be manipulated by other transactions must be
                                        inspected for unintended behavior.
                                    </p>
                                    <div class="vullist clearfix"></div>
                                </div>
                            </div>
                            <div id="vul_list_TODReceiver" class="panel panel-default expanded">
                                <a class="info" data-toggle="modal" data-target="#modal-TODReceiver" href="#">info</a>
                                <div class="panel-heading" data-toggle="collapse" href="#collapse2">
                                    <h4 class="panel-title expand">
                                        <div class="right-arrow pull-right">
                                            <span class="glyphicon glyphicon-menu-up"></span>
                                        </div>
                                        <span>Transactions May Affect Ether Receiver</span>
                                    </h4>
                                </div>
                                <div id="collapse2" class="panel-collapse collapse in" aria-expanded="true">
                                    <p class="penal-item description">
                                        The receiver of ether transfers must not be influenced by other transactions.
                                    </p>
                                    <div class="vullist clearfix"></div>
                                </div>
                            </div>
                            <div id="vul_list_TODAmount" class="panel panel-default expanded">
                                <a class="info" data-toggle="modal" data-target="#modal-TODAmount" href="#">info</a>
                                <div class="panel-heading" data-toggle="collapse" href="#collapse3">
                                    <h4 class="panel-title expand">
                                        <div class="right-arrow pull-right">
                                            <span class="glyphicon glyphicon-menu-up"></span>
                                        </div>
                                        <span>Transactions May Affect Ether Amount</span>
                                    </h4>
                                </div>
                                <div id="collapse3" class="panel-collapse collapse in" aria-expanded="true">
                                    <p class="penal-item description">
                                        The amount of ether transfered must not be influenced by other transactions.
                                    </p>
                                    <div class="vullist clearfix"></div>
                                </div>
                            </div>
                            <div class="report-subcontainer">
                                <hr class="report-hr--seperator">
                                <span class="report-subcontainer-text">Recursive Calls</span>
                                <span class="warning-text total-count recursive-calls-count"></span>
                            </div>
                            <div id="vul_list_DAO" class="panel panel-default expanded">
                                <a class="info" data-toggle="modal" data-target="#modal-DAO" href="#">info</a>
                                <div class="panel-heading" data-toggle="collapse" href="#collapse4">
                                    <h4 class="panel-title expand">
                                        <div class="right-arrow pull-right">
                                            <span class="glyphicon glyphicon-menu-up"></span>
                                        </div>
                                        <span>Gas-dependent Reentrancy</span>
                                    </h4>
                                </div>
                                <div id="collapse4" class="panel-collapse collapse in" aria-expanded="true">
                                    <p class="penal-item description">
                                        Calls into exterernal contracts that receive all remaining gas must not be
                                        followed
                                        by writes to storage.
                                    </p>
                                    <div class="vullist clearfix"></div>
                                </div>
                            </div>
                            <div id="vul_list_DAOConstantGas" class="panel panel-default expanded">
                                <a class="info" data-toggle="modal" data-target="#modal-DAOConstantGas"
                                   href="#">info</a>
                                <div class="panel-heading" data-toggle="collapse" href="#collapse5">
                                    <h4 class="panel-title expand">
                                        <div class="right-arrow pull-right">
                                            <span class="glyphicon glyphicon-menu-up"></span>
                                        </div>
                                        <span>Reentrancy with constant gas</span>
                                    </h4>
                                </div>
                                <div id="collapse5" class="panel-collapse collapse in" aria-expanded="true">
                                    <p class="penal-item description">
                                        Ether transfers (such as send/transfer) must not be followed by writes to
                                        storage.
                                    </p>
                                    <div class="vullist clearfix"></div>
                                </div>
                            </div>
                            <div id="vul_list_DAOMethodCall" class="panel panel-default expanded">
                                <a class="info" data-toggle="modal" data-target="#modal-DAOMethodCall" href="#">info</a>
                                <div class="panel-heading" data-toggle="collapse" href="#collapse6">
                                    <h4 class="panel-title expand">
                                        <div class="right-arrow pull-right">
                                            <span class="glyphicon glyphicon-menu-up"></span>
                                        </div>
                                        <span>Reentrant method call</span>
                                    </h4>
                                </div>
                                <div id="collapse6" class="panel-collapse collapse in" aria-expanded="true">
                                    <p class="penal-item description">
                                        Method calls must not be followed by state changes.
                                    </p>
                                    <div class="vullist clearfix"></div>
                                </div>
                            </div>
                            <div class="report-subcontainer">
                                <hr class="report-hr--seperator">
                                <span class="report-subcontainer-text">Insecure Coding Patterns</span>
                                <span class="warning-text total-count insecure-coding-pattern-count"></span>
                            </div>
                            <div id="vul_list_UnrestrictedWrite" class="panel panel-default expanded">
                                <a class="info" data-toggle="modal" data-target="#modal-UnrestrictedWrite"
                                   href="#">info</a>
                                <div class="panel-heading" data-toggle="collapse" href="#collapse7">
                                    <h4 class="panel-title expand">
                                        <div class="right-arrow pull-right">
                                            <span class="glyphicon glyphicon-menu-up"></span>
                                        </div>
                                        <span>Unrestricted write to storage</span>
                                    </h4>
                                </div>
                                <div id="collapse7" class="panel-collapse collapse in" aria-expanded="true">
                                    <p class="penal-item description">
                                        Contract fields that can be modified by any user must be inspected.
                                    </p>
                                    <div class="vullist clearfix"></div>
                                </div>
                            </div>
                            <div id="vul_list_UnhandledException" class="panel panel-default expanded">
                                <a class="info" data-toggle="modal" data-target="#modal-UnhandledException"
                                   href="#">info</a>
                                <div class="panel-heading" data-toggle="collapse" href="#collapse8">
                                    <h4 class="panel-title expand">
                                        <div class="right-arrow pull-right">
                                            <span class="glyphicon glyphicon-menu-up"></span>
                                        </div>
                                        <span>Unhandled Exception</span>
                                    </h4>
                                </div>
                                <div id="collapse8" class="panel-collapse collapse in" aria-expanded="true">
                                    <p class="penal-item description">
                                        The return value of statements that may return error values must be explicitly
                                        checked.
                                    </p>
                                    <div class="vullist clearfix"></div>
                                </div>
                            </div>
                            <div id="vul_list_DivisionBeforeMultiply" class="panel panel-default expanded">
                                <a class="info" data-toggle="modal" data-target="#modal-DivisionBeforeMultiply"
                                   href="#">info</a>
                                <div class="panel-heading" data-toggle="collapse" href="#collapse9">
                                    <h4 class="panel-title expand">
                                        <div class="right-arrow pull-right">
                                            <span class="glyphicon glyphicon-menu-up"></span>
                                        </div>
                                        <span>Division Before Multiplication</span>
                                    </h4>
                                </div>
                                <div id="collapse9" class="panel-collapse collapse in" aria-expanded="true">
                                    <p class="penal-item description">
                                        The use of division before multiplication may result in incorrect final results
                                        due
                                        to integer rounding.
                                    </p>
                                    <div class="vullist clearfix"></div>
                                </div>
                            </div>
                            <div id="vul_list_DivisionBeforeCallvalue" class="panel panel-default expanded">
                                <a class="info" data-toggle="modal" data-target="#modal-DivisionBeforeCallvalue"
                                   href="#">info</a>
                                <div class="panel-heading" data-toggle="collapse" href="#collapse10">
                                    <h4 class="panel-title expand">
                                        <div class="right-arrow pull-right">
                                            <span class="glyphicon glyphicon-menu-up"></span>
                                        </div>
                                        <span>Division influences Transfer Amount</span>
                                    </h4>
                                </div>
                                <div id="collapse10" class="panel-collapse collapse in" aria-expanded="true">
                                    <p class="penal-item description">
                                        The use of division to calculate the amount of transfered ether may be incorrect
                                        due
                                        to integer rounding.
                                    </p>
                                    <div class="vullist clearfix"></div>
                                </div>
                            </div>
                            <div id="vul_list_UnprivilegedSuicide" class="panel panel-default expanded">
                                <a class="info" data-toggle="modal" data-target="#modal-UnprivilegedSuicide"
                                   href="#">info</a>
                                <div class="panel-heading" data-toggle="collapse" href="#collapse11">
                                    <h4 class="panel-title expand">
                                        <div class="right-arrow pull-right">
                                            <span class="glyphicon glyphicon-menu-up"></span>
                                        </div>
                                        <span>Unrestricted Selfdestruct</span>
                                    </h4>
                                </div>
                                <div id="collapse11" class="panel-collapse collapse in" aria-expanded="true">
                                    <p class="penal-item description">
                                        The execution of selfdestrruct statements must be restricted to an authorized
                                        set of
                                        users.
                                    </p>
                                    <div class="vullist clearfix"></div>
                                </div>
                            </div>
                            <div id="vul_list_MissingInputValidation" class="panel panel-default expanded">
                                <a class="info" data-toggle="modal" data-target="#modal-MissingInputValidation"
                                   href="#">info</a>
                                <div class="panel-heading" data-toggle="collapse" href="#collapse12">
                                    <h4 class="panel-title expand">
                                        <div class="right-arrow pull-right">
                                            <span class="glyphicon glyphicon-menu-up"></span>
                                        </div>
                                        <span>Missing Input Validation</span>
                                    </h4>
                                </div>
                                <div id="collapse12" class="panel-collapse collapse in" aria-expanded="true">
                                    <p class="penal-item description">
                                        Method arguments must be sanitized before they are used in computations.
                                    </p>
                                    <div class="vullist clearfix"></div>
                                </div>
                            </div>
                            <div id="vul_list_UseOfOrigin" class="panel panel-default expanded">
                                <a class="info" data-toggle="modal" data-target="#modal-UseOfOrigin" href="#">info</a>
                                <div class="panel-heading" data-toggle="collapse" href="#collapse13">
                                    <h4 class="panel-title expand">
                                        <div class="right-arrow pull-right">
                                            <span class="glyphicon glyphicon-menu-up"></span>
                                        </div>
                                        <span>Use Of Origin</span>
                                    </h4>
                                </div>
                                <div id="collapse13" class="panel-collapse collapse in" aria-expanded="true">
                                    <p class="penal-item description">
                                        The origin statement must not be used.
                                    </p>
                                    <div class="vullist clearfix"></div>
                                </div>
                            </div>
                            <div class="report-subcontainer">
                                <hr class="report-hr--seperator">
                                <span class="report-subcontainer-text">Unexpected Ether Flows</span>
                                <span class="warning-text total-count unexpected-ether-flows-count"></span>
                            </div>
                            <div id="vul_list_UnrestrictedEtherFlow" class="panel panel-default expanded">
                                <a class="info" data-toggle="modal" data-target="#modal-UnrestrictedEtherFlow"
                                   href="#">info</a>
                                <div class="panel-heading" data-toggle="collapse" href="#collapse14">
                                    <h4 class="panel-title expand">
                                        <div class="right-arrow pull-right">
                                            <span class="glyphicon glyphicon-menu-up"></span>
                                        </div>
                                        <span>Unrestricted ether flow</span>
                                    </h4>
                                </div>
                                <div id="collapse14" class="panel-collapse collapse in" aria-expanded="true">
                                    <p class="penal-item description">
                                        The execution of ether flows should be restricted to an authorized set of users.
                                    </p>
                                    <div class="vullist clearfix"></div>
                                </div>
                            </div>
                            <div id="vul_list_LockedEther" class="panel panel-default expanded">
                                <a class="info" data-toggle="modal" data-target="#modal-LockedEther" href="#">info</a>
                                <div class="panel-heading" data-toggle="collapse" href="#collapse15">
                                    <h4 class="panel-title expand">
                                        <div class="right-arrow pull-right">
                                            <span class="glyphicon glyphicon-menu-up"></span>
                                        </div>
                                        <span>Locked Ether</span>
                                    </h4>
                                </div>
                                <div id="collapse15" class="panel-collapse collapse in" aria-expanded="true">
                                    <p class="penal-item description">
                                        Contracts that may receive ether must also allow users to extract the deposited
                                        ether from the contract.
                                    </p>
                                    <div class="vullist clearfix"></div>
                                </div>
                            </div>
                            <div class="report-subcontainer">
                                <hr class="report-hr--seperator">
                                <span class="report-subcontainer-text">Dependance on unsafe inputs</span>
                                <span class="warning-text total-count dependance-on-unsafe-inputs-count"></span>
                            </div>
                            <div id="vul_list_UnsafeDependanceOnBlock" class="panel panel-default expanded">
                                <a class="info" data-toggle="modal" data-target="#modal-UnsafeDependanceOnBlock"
                                   href="#">info</a>
                                <div class="panel-heading" data-toggle="collapse" href="#collapse16">
                                    <h4 class="panel-title expand">
                                        <div class="right-arrow pull-right">
                                            <span class="glyphicon glyphicon-menu-up"></span>
                                        </div>
                                        <span>Unsafe Dependance On Block Information</span>
                                    </h4>
                                </div>
                                <div id="collapse16" class="panel-collapse collapse in" aria-expanded="true">
                                    <p class="penal-item description">
                                        Security-sensitive operations must not depend on block information.
                                    </p>
                                    <div class="vullist clearfix"></div>
                                </div>
                            </div>
                            <div id="vul_list_UnsafeDependanceOnGas" class="panel panel-default expanded">
                                <a class="info" data-toggle="modal" data-target="#modal-UnsafeDependanceOnGas"
                                   href="#">info</a>
                                <div class="panel-heading" data-toggle="collapse" href="#collapse17">
                                    <h4 class="panel-title expand">
                                        <div class="right-arrow pull-right">
                                            <span class="glyphicon glyphicon-menu-up"></span>
                                        </div>
                                        <span>Unsafe Dependance On Block Gas</span>
                                    </h4>
                                </div>
                                <div id="collapse17" class="panel-collapse collapse in" aria-expanded="true">
                                    <p class="penal-item description">
                                        Security-sensitive operations must not depend on gas-related information.
                                    </p>
                                    <div class="vullist clearfix"></div>
                                </div>
                            </div>
                            <div id="vul_list_DelegateCallWithUserInput" class="panel panel-default expanded">
                                <a class="info" data-toggle="modal" data-target="#modal-DelegateCallWithUserInput"
                                   href="#">info</a>
                                <div class="panel-heading" data-toggle="collapse" href="#collapse18">
                                    <h4 class="panel-title expand">
                                        <div class="right-arrow pull-right">
                                            <span class="glyphicon glyphicon-menu-up"></span>
                                        </div>
                                        <span>Delegatecall dependent on user input</span>
                                    </h4>
                                </div>
                                <div id="collapse18" class="panel-collapse collapse in" aria-expanded="true">
                                    <p class="penal-item description">
                                        The target and arguments provided to delegatecall must be sanitized.
                                    </p>
